
{
    
    "head": {

        "type": "UIViewController",

        "script": [
            {
                "path": "json/ThridTab/ThridTab.js"
            }
        ]

    },


    "content": {

        "title": "Thrid",


        "subviews": [

            {
                "class_name": "UITableView",
                "var_name": "testTableView",

                // 如果是表视图类，则有此Key设定表视图样式。(plain:普通样式, grouped:分组模式)
                "style": "grouped",

                // plain 样式时，做自定义 cell 样式
                "cell": {

                    "type": "original",

                    // type 为 custom 则给出自定义 单元的 样式 json 文件
                    //"path": "somePath/CustomTableCell.json",

                    "height": "44"
                    
                },

                "sectionHeader": {
                    "type": "original",

                    // type 为 custom 则给出自定义 单元头的 样式 json 文件
                    //"path": "somePath/CustomTableSectionHeader.json",
                    "height": "24"
                },

                // 表视图数据源
                "dataSource": {
                    // 本地数据源 (或 http 请求到的 json对象)
                    "type": "local",

                    // json 文件路径
                    "path": "json/ThridTab/ThridTab_testTableViewDataSource.json"

                },

                // 点击行事件
                "action": "json/ThridTab/ThridTab_testTableViewActions.js"
                    
            }

        ]

    } 

    


}







